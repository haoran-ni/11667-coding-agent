{   
    "model_name": "placeholder",
    "dataset_name": "iamtarun/python_code_instructions_18k_alpaca",
    "dataset_split": "train",
    "attn_implementation": "eager",
    "hp_tuning": false,

    "load_in_4bit": true,
    "bnb_4bit_quant_type": "nf4",
    "bnb_4bit_compute_dtype": "bfloat16",
    "bnb_4bit_use_double_quant": false,

    "lora_r": 64,
    "lora_alpha": 16,
    "lora_dropout": 0.05,
    "lora_bias": "none",
    "lora_task_type": "CAUSAL_LM",

    "sft_max_seq_length": 2048,
    "sft_packing": false,

    "output_dir": "./models/evaluate/placeholder",
    "run_name": "placeholder",
    "report_to": "none",
    "eval_strategy": "no",
    "logging_steps": 1,
    "save_strategy": "no",
    "bf16": true,
    "gradient_checkpointing": true,
    "deepspeed": null,
    "seed": 42,
    "group_by_length": true,
    "disable_tqdm": false,

    "num_train_epochs": 1,
    "gradient_accumulation_steps": 1,
    "per_device_train_batch_size": 8,
    "per_device_eval_batch_size": 8,
    "optim": "adamw_torch",
    "learning_rate": 1e-5,
    "weight_decay": 0.001,
    "warmup_ratio": 0.03,
    "lr_scheduler_type": "cosine",
    "max_grad_norm": 0.3,

    "eval_on_validation": true,
    "is_unmerged": true,
    "is_bnb": true,
    "ppl_only": true,
    "legacy_generation": false
}