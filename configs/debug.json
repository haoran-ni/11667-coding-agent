{   
    "model_name": "caizefeng18/llama-3.2-3b-instruct-python-alpaca-finetune-QLoRA-debug",
    "dataset_name": "iamtarun/python_code_instructions_18k_alpaca",
    "dataset_split": "train",
    "attn_implementation": "eager",
    "hp_tuning": false,

    "load_in_4bit": true,
    "bnb_4bit_quant_type": "nf4",
    "bnb_4bit_compute_dtype": "bfloat16",
    "bnb_4bit_use_double_quant": true,

    "lora_r": 64,
    "lora_alpha": 16,
    "lora_dropout": 0.05,
    "lora_bias": "none",
    "lora_task_type": "CAUSAL_LM",

    "sft_max_seq_length": 2048,
    "sft_packing": false,

    "output_dir": "./models/evaluate/placeholder_22",
    "run_name": "placeholder",
    "report_to": "none",
    "eval_strategy": "steps",
    "eval_steps": 50,
    "eval_on_start": true,
    "logging_steps": 1,
    "save_strategy": "steps",
    "save_steps": 50,
    "bf16": true,
    "gradient_checkpointing": true,
    "deepspeed": null,
    "seed": 42,
    "group_by_length": true,
    "disable_tqdm": false,

    "num_train_epochs": 0.1,
    "gradient_accumulation_steps": 2,
    "per_device_train_batch_size": 4,
    "per_device_eval_batch_size": 4,
    "optim": "paged_adamw_32bit",
    "learning_rate": 1e-5,
    "weight_decay": 0.001,
    "warmup_ratio": 0.03,
    "lr_scheduler_type": "cosine",
    "max_grad_norm": 0.3
}